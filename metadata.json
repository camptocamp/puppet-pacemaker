{
  "name": "camptocamp-pacemaker",
  "version": "1.2.1",
  "author": "Camptocamp",
  "summary": "Puppet Pacemaker module",
  "license": "Apache-2.0",
  "source": "https://github.com/camptocamp/puppet-pacemaker",
